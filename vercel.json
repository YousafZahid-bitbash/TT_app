{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist"
        }
      },
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ],
    "functions": {
      "api/main.py": {
        "runtime": "python3.9"
      }
    }
  }